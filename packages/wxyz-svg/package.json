{
  "name": "@deathbeds/wxyz-svg",
  "version": "0.6.0",
  "description": "experimental Jupyter widgets for SVG",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyter-widgets",
    "widgets"
  ],
  "homepage": "https://deathbeds.github.io/wxyz",
  "bugs": {
    "url": "https://github.com/deathbeds/wxyz/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/wxyz"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "wxyz contributors",
    "email": ""
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "{src,lib,style}/**/*.{js,ts,svg,json,css,map,ts,tsx}",
    "LICENSE.txt"
  ],
  "dependencies": {
    "@deathbeds/wxyz-core": "^0.6.0",
    "d3-drag": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-zoom": "^2.0.0"
  },
  "devDependencies": {
    "@jupyter-widgets/base": "6",
    "@jupyter-widgets/controls": "5",
    "@jupyter-widgets/jupyterlab-manager": "5",
    "@jupyterlab/builder": "^3.5.0",
    "@jupyterlab/rendermime": "3",
    "@types/d3-drag": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-zoom": "^2.0.0",
    "@types/node": "^10.11.6"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "discovery": {
      "server": {
        "base": {
          "name": "wxyz_svg"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "outputDir": "../src/wxyz_svg/src/svg/_d/share/jupyter/labextensions/@deathbeds/wxyz-svg",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyter-widgets/controls": {
        "bundled": false,
        "singleton": true
      },
      "@deathbeds/wxyz-core": {
        "bundled": false,
        "singleton": true
      },
      "d3-zoom": {
        "bundled": true,
        "singleton": true
      },
      "d3-selection": {
        "bundled": true,
        "singleton": true
      },
      "d3-drag": {
        "bundled": true,
        "singleton": true
      }
    }
  }
}
