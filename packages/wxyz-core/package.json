{
  "name": "@deathbeds/wxyz-core",
  "version": "0.6.0",
  "description": "Experimental Jupyter widgets for JSON and evented transformations",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyter-widgets",
    "widgets"
  ],
  "homepage": "https://deathbeds.github.io/wxyz",
  "bugs": {
    "url": "https://github.com/deathbeds/wxyz/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/wxyz"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "wxyz contributors",
    "email": ""
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "{src,lib,style}/**/*.{js,ts,svg,json,css,map,ts,tsx}",
    "LICENSE.txt"
  ],
  "dependencies": {
    "jsonpointer": "^4.1.0"
  },
  "devDependencies": {
    "@jupyter-widgets/base": "6",
    "@jupyter-widgets/controls": "5",
    "@jupyter-widgets/jupyterlab-manager": "5",
    "@jupyterlab/builder": "^3.5.0",
    "@jupyterlab/rendermime": "3",
    "@types/node": "^10.11.6",
    "ajv": "*"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "webpackConfig": "./webpack.config.js",
    "discovery": {
      "server": {
        "base": {
          "name": "wxyz_core"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "outputDir": "../../src/wxyz_core/src/_d/share/jupyter/labextensions/@deathbeds/wxyz-core",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyter-widgets/controls": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
