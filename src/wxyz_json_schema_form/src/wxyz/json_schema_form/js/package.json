{
  "author": {
    "email": "",
    "name": "wxyz contributors"
  },
  "bugs": {
    "url": "https://github.com/deathbeds/wxyz/issues"
  },
  "dependencies": {
    "@deathbeds/jupyterlab-rjsf": "^1.0.2"
  },
  "description": "experimental Jupyter widgets for JSON Schema Form",
  "devDependencies": {
    "@rjsf/core": "~2.4.0",
    "@jupyter-widgets/base": "6",
    "@jupyter-widgets/controls": "5",
    "@jupyter-widgets/jupyterlab-manager": "5",
    "@jupyterlab/builder": "^3.0.1",
    "@deathbeds/wxyz-core": "^0.5.1",
    "@blueprintjs/core": "*",
    "@types/node": "^10.11.6",
    "typescript": "~4.3.2",
    "@types/react-dom": "*"
  },
  "files": [
    "{src,lib,style}/**/*.{js,ts,svg,json,css,map,ts,tsx}",
    "LICENSE.txt"
  ],
  "homepage": "https://deathbeds.github.io/wxyz",
  "jupyterlab": {
    "extension": "lib/plugin",
    "discovery": {
      "server": {
        "base": {
          "name": "wxyz_json_schema_form"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "outputDir": "../labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyter-widgets/controls": {
        "bundled": false,
        "singleton": true
      },
      "@deathbeds/wxyz-core": {
        "bundled": false,
        "singleton": true
      },
      "codemirror": {
        "bundled": false,
        "singleton": true
      },
      "@deathbeds/jupyterlab-rjsf": {
        "bundled": true,
        "singleton": false
      },
      "@rjsf/core": {
        "bundled": true,
        "singleton": false
      }
    }
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyter-widgets",
    "widgets"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "@deathbeds/wxyz-json-schema-form",
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/wxyz"
  },
  "types": "lib/index.d.ts",
  "version": "0.6.0"
}
