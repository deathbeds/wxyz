{
  "author": {
    "email": "",
    "name": "wxyz contributors"
  },
  "bugs": {
    "url": "https://github.com/deathbeds/wxyz/issues"
  },
  "dependencies": {
    "nunjucks": "^3.2.3"
  },
  "description": "experimental Jupyter widgets for Nunjucks templates",
  "devDependencies": {
    "@jupyter-widgets/base": "6",
    "@jupyter-widgets/controls": "5",
    "@jupyter-widgets/jupyterlab-manager": "5",
    "@deathbeds/wxyz-core": "^0.5.1",
    "@jupyterlab/builder": "^3.0.1",
    "@types/node": "^10.11.6",
    "@types/nunjucks": "^3.0.0",
    "typescript": "~4.3.2"
  },
  "files": [
    "{src,lib,style}/**/*.{js,ts,svg,json,css,map,ts,tsx}",
    "LICENSE.txt"
  ],
  "homepage": "https://deathbeds.github.io/wxyz",
  "jupyterlab": {
    "extension": "lib/plugin",
    "discovery": {
      "server": {
        "base": {
          "name": "wxyz_tpl_jinja"
        },
        "managers": [
          "pip"
        ]
      }
    },
    "outputDir": "../labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      },
      "@jupyter-widgets/controls": {
        "bundled": false,
        "singleton": true
      },
      "@deathbeds/wxyz-core": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "jupyter-widgets",
    "widgets"
  ],
  "license": "BSD-3-Clause",
  "main": "lib/index.js",
  "name": "@deathbeds/wxyz-tpl-nunjucks",
  "repository": {
    "type": "git",
    "url": "https://github.com/deathbeds/wxyz"
  },
  "types": "lib/index.d.ts",
  "version": "0.6.0"
}
